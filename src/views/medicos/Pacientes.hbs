<div class="container p-4">
    <div class="row">
        <div class="col-md-1"></div>
        <div class="col-md-11">
            <h1>Pacientes</h1>
            <input class="form-control form-control-md" id="myInput" type="text" placeholder="Buscar..."><br>
        </div>
    </div>
    <div class="row">
        <div class="col-md-1"></div>
        <div class="col-md-11">
                <table class="table table-striped table-hover">
                    <thead class="thead-dark">
                        <tr>
                            <th></th>
                            <th></th>
                            <th></th>
                            <th></th>
                            <th>NSS</th>
                            <th>Nombre</th>
                            <th>Fecha de nacimiento</th>
                            <th>GÃ©nero</th>
                            <th>Correo</th>
                             
                            </tr>
                    </thead>
                    <tbody id="myTable">
                        {{#each consultados}}
                        <tr>
                            <td><a href="/links/verPerfilP/{{this.NSS}}" class="btn btn-success">ver</a></td>
                            <td><a href="/medicos/AgendarCita/{{this.NSS}}" class="btn btn-primary">Agendar Cita</a></td>
                            <td><a href="/medicos/diagnosticar/{{this.NSS}}" class="btn btn-primary">Diagnosticar</a></td>
                            <td><a href="/medicos/Receta/{{this.NSS}}" class="btn btn-info">Recetar</a></td>
                            <td>{{this.NSS}}</td>
                            <td>{{this.NombrePaciente}} {{this.ApellidoPaternoPaciente}} {{this.ApellidoMaternoPaciente}}</td>
                            <td>{{this.FechaNacimientoPaciente}}</td>
                            <td>{{this.SexoPaciente}}</td>
                            <td>{{this.CorreoElectronicoPaciente}}</td>
                             
                        </tr>         {{/each}}           
                    </tbody>
                </table>
            </div>
        </div>
        </div>
    </div>
</div>
<script>
        $(document).ready(function(){
            $("#myInput").on("keyup", function() {
                var value = $(this).val().toLowerCase();
                $("#myTable tr").filter(function() {
                    $(this).toggle($(this).text().toLowerCase().indexOf(value) > -1)
                });
            });
        });
    </script>