<div class="container">
          <h1 style="text-align: center;">Fichas médicas</h1><br><br>
          <div class="row">
                <div class="col-md-11">
            

            <input class="form-control form-control-md" id="myInput" type="text" placeholder="Buscar...">
            <br>
                <table class="table table-striped table-hover">
                    <thead class="thead-dark">
                        <tr>
                            <th></th>
                            <th>Fecha</th>
                            <th>Cédula Médico</th>
                            <th>Nombre médico</th>
                            <th>Peso Paciente</th>
                            <th>Estatura Paciente</th>
                            
                            </tr>
                    </thead>
                    <tbody id="myTable">
                        {{#each links}}
                        <tr>
                            <td><a href="/links/verFicha/{{this.FichaMedica}}" class="btn btn-primary">Ver</a></td>  
                            <td>{{this.fecha}} </td>
                            <td>{{this.CedulaProfesional}}</td> 
                            <td>{{this.NombreMedico}} {{this.ApellidoPaternoMedico}} {{this.ApellidoMaternoMedico}}</td>
                            <td>{{peso}} kg</td>
                            <td>{{estatura}} cm</td>
                        </tr>     
                        
                        {{/each}}               
                    </tbody>
                </table>
                 <br>
                 <a href="/links/verPerfilP/{{linkss.NSS}}" class="btn btn-info">Regresar</a>
            </div>
          </div>
      </div>